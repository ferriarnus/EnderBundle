{
  "schemaVersion": 1,
  "id": "enderbundle",
  "version": "${version}",

  "name": "Ender Bundle",
  "description": "An Ender Bundle for inventory mangement",
  "authors": [
    "Ferri Arnus"
  ],
  "contact": {
    "homepage": "",
    "sources": ""
  },

  "license": "CC0-1.0",
  "icon": "assets/enderbundle/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.ferri.arnus.enderbundle.EnderBundleMain"
    ],
    "client": [
      {
        "value": "com.ferri.arnus.enderbundle.ClientEnderBundleMain"
      }
    ],
    "cardinal-components-item": [
      "com.ferri.arnus.enderbundle.components.MyComponents"
    ]
  },
  "mixins": [
    "enderbundle.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  },
  "custom": {
        "cardinal-components": [
            "enderbundle:enderstorage",
            "enderbundle:color"
        ]
    }
}
